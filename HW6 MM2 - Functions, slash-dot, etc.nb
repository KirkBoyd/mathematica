   RowBox[{"(", 
    RowBox[{"a", " ", "+", " ", 
     RowBox[{"b", " ", "I"}]}], ")"}], "/", 
   RowBox[{"(", 
    RowBox[{"c", " ", "-", " ", 
     RowBox[{"d", " ", "I"}]}], ")"}]}]}], "\[IndentingNewLine]", 

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"(", "a", ")"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"g", "[", "x", "]"}], "=", 
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{"3", " ", 
        RowBox[{"(", 
         RowBox[{"1", "+", "x"}], ")"}]}], ")"}]}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "1"}], " ", "+", " ", 
        RowBox[{"2", "x"}]}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"6", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", "x", " ", "+", 
          RowBox[{"x", "^", "2"}]}], ")"}]}], ")"}]}], "+", 
     RowBox[{"2", "/", 
      RowBox[{"(", 
       RowBox[{"3", 

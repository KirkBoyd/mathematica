    RowBox[{
     RowBox[{
      RowBox[{"x", " ", "+", " ", 
       RowBox[{"2", " ", "y"}]}], " ", "\[Equal]", " ", "3"}], " ", ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"4", " ", "x"}], " ", "+", " ", 
       RowBox[{"5", " ", "y"}]}], " ", "\[Equal]", "6"}]}], " ", "}"}], ",", 
